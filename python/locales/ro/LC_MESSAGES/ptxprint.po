#: lib/ptxprint/ptxprint.glade:13310 lib/ptxprint/ptxprint.glade:14797
#: lib/ptxprint/ptxprint.glade:15330
msgid "Indent (mm)"
msgstr "Indentare (mm)"

#: lib/ptxprint/ptxprint.glade:13327 lib/ptxprint/ptxprint.glade:14814
#: lib/ptxprint/ptxprint.glade:15347
