msgid "Scale"
msgstr "المقياس"

#: lib/ptxprint/ptxprint.glade:9535 lib/ptxprint/ptxprint.glade:11605
msgid "Posn"
msgstr "ملصق"

#: lib/ptxprint/ptxprint.glade:9549 lib/ptxprint/ptxprint.glade:11619
msgid "Ref"
msgstr "مرجع"

#: lib/ptxprint/ptxprint.glade:9563 lib/ptxprint/ptxprint.glade:11633
msgid "Mirror"
msgstr "مرآة"

