msgstr "148mm, 210mm (A5)"

#: lib/ptxprint/ptxprint.glade:9170
msgid "176mm, 250mm (B5)"
msgstr "176mm, 250mm (B5)"

#: lib/ptxprint/ptxprint.glade:9171
msgid "8.5in, 11in (Letter)"
msgstr "8.5in, 11in (Letter)"

#: lib/ptxprint/ptxprint.glade:9172
msgid "5.5in, 8.5in (1/2 Letter)"
msgstr "5.5in, 8.5in (1/2 Letter)"

#: lib/ptxprint/ptxprint.glade:9173
